  "apparmor": "true",
  "ingress": "true",
  "stage": "experimental",
  "devices": ["/dev/sda1:/dev/sda1:rwm","/dev/sdb1:/dev/sdb1:rwm","/dev/mmcblk0p1:/dev/mmcblk0p1:rwm"],
